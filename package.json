    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.3.0",
    "msw": "^2.9.0",
    "playwright": "^1.49.1",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}",
      "lib/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ]
